<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'⌘C'</ShortCut>
          <Description>Copy</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>120</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste">
        <Item>
          <IdeaActionID>$Paste</IdeaActionID>
          <ShortCut>'⌘V'</ShortCut>
          <Description>Paste</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste from History...">
        <Item>
          <IdeaActionID>PasteMultiple</IdeaActionID>
          <ShortCut>'⇧⌘V'</ShortCut>
          <Description>Paste from History...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Unnamed'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⌃R'</ShortCut>
          <Description>Run 'Unnamed'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'counter.spec.js'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⌃R'</ShortCut>
          <Description>Run 'counter.spec.js'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Start Listening for PHP Debug Connections">
        <Item>
          <IdeaActionID>PhpListenDebugAction</IdeaActionID>
          <ShortCut>'⌃D'</ShortCut>
          <Description>Start Listening for PHP Debug Connections</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'All  Tests'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'All  Tests'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop Listening for PHP Debug Connections">
        <Item>
          <IdeaActionID>PhpListenDebugAction</IdeaActionID>
          <ShortCut>'⌃D'</ShortCut>
          <Description>Stop Listening for PHP Debug Connections</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'⌘2'</ShortCut>
          <Description>Terminal</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>